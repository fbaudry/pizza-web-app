<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets
            'css/bootstrap.min.css'
            'css/animate.css'
            'css/font-awesome.min.css'
            'css/ionicons.min.css'
            'css/main.css'
            'css/responsive.css'
            'css/style.css'
            filter="cssrewrite" %}
            <link rel="stylesheet" href="{{ asset_url }}"   />
            {% endstylesheets %}

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block js %}{% endblock %}
    </body>
</html>
